-- Scripts:

local function LGDQB_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local Player = game.Players.LocalPlayer
	local rank = Player:GetRankInGroup(13421844)
	
	if rank >= 250 then
		script.Parent.Parent.Toggle.Visible = true
		script.Parent.ClearData.TextButton.Activated:Connect(function()
			game.ReplicatedStorage.Remotes.Admin:FireServer("WipeData",game.Players.LocalPlayer.UserId)
		end)
		script.Parent.InstantXP.TextButton.Activated:Connect(function()
			game.ReplicatedStorage.Remotes.Admin:FireServer("GiveXP")
		end)
		script.Parent.GiveCoins.TextButton.Activated:Connect(function()
			game.ReplicatedStorage.Remotes.Admin:FireServer("GiveCoins")
		end)
		script.Parent.ShutDown.TextButton.Activated:Connect(function()
			game.ReplicatedStorage.Remotes.Admin:FireServer("ServerShutdown")
		end)
		script.Parent.Parent.Toggle.Activated:Connect(function()
			script.Parent.Visible = not script.Parent.Visible
		end)
	else
		script.Parent.Parent:Destroy()
	end
end
coroutine.wrap(LGDQB_fake_script)()
local function AEMFY_fake_script() -- Version.LocalScript 
	local script = Instance.new('LocalScript', Version)

	script.Parent.Text = game.Workspace.Parent.Name
end
coroutine.wrap(AEMFY_fake_script)()
local function XCHOR_fake_script() -- AdminPanel.Admin 
	local script = Instance.new('Script', AdminPanel)

	local remote = game.ReplicatedStorage:WaitForChild("Remotes"):WaitForChild("Admin")
	local DataHandler = require(game.ServerStorage.Modules.DataHandler)
	
	
	function cmd(player,command,data)
		local rank = player:GetRankInGroup(13421844)
		if rank >= 250 then
			if command == "WipeData" then
				local ProfileService = require(game.ServerStorage.Modules.DataHandler.ProfileService)
				local GameProfileStore = ProfileService.GetProfileStore("PlayerData",{})
				GameProfileStore:WipeProfileAsync("Player_"..data)
				player:Kick("Data Wiped")
			end
			if command == "GiveXP" then
				DataHandler.IncrementData(player,"BakesThisLevel",5)
			end
			if command == "GiveCoins" then
				DataHandler.IncrementData(player,"Cash",1000000)
			end
			if command == "ServerShutdown" then
				if rank>=254 then
					game.Players.ChildAdded:connect(function(h)
						h:Kick("Server Shutdown, Please Rejoin")
					end)
					for _,i in pairs (game.Players:GetChildren()) do
						i:Kick("Server Shutdown, Please Rejoin")
					end
				end
			end
			else
				player:Kick("Uh oh! Stinky!")
			end
		end
	
		remote.OnServerEvent:Connect(cmd)
	
end
coroutine.wrap(XCHOR_fake_script)()
